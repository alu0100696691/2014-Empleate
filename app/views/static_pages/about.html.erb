<% provide(:title, 'ACERCA DE') %>
<div class="container">
                <div class="row">
                        <div class="header col-xs-12 col-sm-12 col-md-12 ">
				<strong><h1>Acerca de</h1></strong>
			<div class="center hero-unit">	
			<%= image_tag "empleate.png", :alt => "logo empleate", :title => "empleate", :class => "img-circle" %>
			</div>
			<br/><br/></div>
		</div>          
		
		<div class="row">
                	<div class="header col-xs-12 col-sm-12 col-md-12 ">
